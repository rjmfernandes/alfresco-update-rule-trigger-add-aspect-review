<beans>
    
    
    <!--  test beans . only for tests purposes -->
        <!-- Registration of new models --> 
    <bean id="test.extension.dictionaryBootstrap" parent="dictionaryModelBootstrap" depends-on="dictionaryBootstrap">
        <property name="models">
            <list>
                <value>alfresco/module/${artifactId}/test-model.xml</value>
            </list>
        </property>
    </bean>
    
    <bean id="testrule.bootstrap" 
      class="org.alfresco.repo.module.ImporterModuleComponent" 
      parent="module.baseComponent">
   <!-- Module Details -->
   <property name="moduleId" value="${artifactId}" />
   <property name="name" value="${artifactId}" />
   <!-- Data properties -->
   <property name="importer" ref="spacesBootstrap"/>
   <property name="bootstrapViews">
      <list>
         <props>
            <prop key="path">/${spaces.company_home.childname}</prop>
            <prop key="location">alfresco/module/${artifactId}/testrulespace.acp</prop>
         </props>
      </list>
   </property>

</bean>
    
</beans>
